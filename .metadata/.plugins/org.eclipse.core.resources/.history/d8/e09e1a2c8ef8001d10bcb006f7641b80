#include <stdio.h>

int main() {
    float total_mark;

    printf("Enter the student's Total marks: ");
    scanf("%f", &mark);

    if (mark >= 90) {
        printf("Grade: A\n");
    } else if (mark >= 80 && mark <= 89) {
        printf("Grade: B\n");
    } else if (mark >= 70 && mark <= 79) {
        printf("Grade: C\n");
    } else if (mark >= 60 && mark <= 69) {
        printf("Grade: D\n");
    } else if (mark >= 50 && mark <= 59) {
        printf("Grade: E\n");
    } else {
        printf("Grade: Failed\n");
    }

    return 0;
}
