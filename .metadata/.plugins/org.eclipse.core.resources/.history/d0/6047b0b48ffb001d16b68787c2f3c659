#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i, j, a[100], size, k;
	setbuf(stdout, NULL);
	printf("enter the size");
	scanf("%d", &size);
	printf("enter the values");
	for (i = 0; i < size; i++) {
		scanf("%d", &a[i]);
	}
	for (i = 0; i < size; i++) {
		for (int j = i + 1; j < size; j++) {
			if (a[i] % 5 == 0) {
				for (int k = j; k < size; k++) {
					a[k] = a[k + 1];
					size--;
					j--;
				}
			}

		}
	}
}
printf("deleted values");
for(i=0;i<size;i++)
{
	printf("%d ",a[i]);
}

return EXIT_SUCCESS;
}
