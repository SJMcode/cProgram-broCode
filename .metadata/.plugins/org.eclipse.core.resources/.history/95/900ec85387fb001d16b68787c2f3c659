#include <stdio.h>
#include <stdlib.h>

int main(void) {


	int n,i, a[100],b[100];
	setbuf(stdout, NULL);

		printf("Enter limit\n");
		scanf("%d", &n);

		printf("Enter numbers\n");
		for(i=0; i<n; i++){

			scanf("%d", &a[i]);
		}


		for(i=n-1; i>=0; i--){
			printf("%d ", a[i]);
		}

	return EXIT_SUCCESS;
}
