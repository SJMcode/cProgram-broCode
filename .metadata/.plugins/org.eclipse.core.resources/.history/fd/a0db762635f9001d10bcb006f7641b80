int main(void) {

	//char str[] = {'M','A', 'L', 'A', 'Y', 'A', 'L', 'A', 'M'};
	char str[20];
	char str1[20];
	int i;
	printf("Enter the string: ");
	scanf("%s", str);
	strcpy(str, str1);
	strrev(str1);
	if(str = str1)
		printf("Palindrome");
	else
		printf("Not palindrome");

	return EXIT_SUCCESS;
}
