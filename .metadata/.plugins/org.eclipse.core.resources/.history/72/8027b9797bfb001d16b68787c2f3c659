#include <stdio.h>
#include <stdlib.h>

int main(void) {


	int n,i, a[100];
	setbuf(stdout, NULL);

		printf("Enter limit\n");
		scanf("%d", &n);

		printf("Enter numbers\n");
		for(i=0; i<n; i++){

			scanf("%d", &a[i]);
		}

		for(int k=0; k<n-1; k++){
			a[k] = a[n-1];
			k++;
			n--;
		}
		for(i=0; i<n; i++){

					printf("%d "
							"", a[i]);
				}


	return EXIT_SUCCESS;
}
