#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i,j,n;
	int a[100];
	setbuf(stdout, NULL);

	printf("Enter the array limit: ");
	scanf("%d", &n);
	//n=sizeof(a)/sizeof(a[0]);

	for(int i=0; i<n; i++){
		scanf("%d", a[i]);
	}
	for(int i=0; i<n; i++){
			printf("%d ", a[i]);
		}

	return EXIT_SUCCESS;
}
